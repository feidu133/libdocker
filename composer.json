{
  "name": "khs1994/docker",
  "description": "A PHP Library for the Docker Engine API",
  "keywords": [
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/khs1994-docker/libdocker",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "support": {
    "issues": "https://github.com/khs1994-docker/libdocker/issues"
  },
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-curl": "*",
    "pimple/pimple": "~3.0",
    "khs1994/curl": "~18.06.0"
  },
  "require-dev": {
    "illuminate/console": "^5.5",
    "phpunit/phpunit": "^7.1"
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "suggest": {
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "Docker\\": "src/Docker/"
    },
    "files": ["src/Docker/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Docker\\Tests\\": "tests/Docker/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    },
    "laravel": {
      "providers": [
        "Docker\\ServiceProvider"
      ],
      "aliases": {
        "Docker": "Docker\\Facade"
      }
    }
  }
}
